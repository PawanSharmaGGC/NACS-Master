@using Kentico.PageBuilder.Web.Mvc
@using Kentico.Web.Mvc

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <link rel="icon" href="/favicon.png" />
    <meta name="viewport" content="width=device-width,initial-scale=1" />
    <meta name="theme-color" content="#000000" />
    <meta name="description" content="Web site created using create-react-app" />
    <link rel="apple-touch-icon" href="/logo192.png" />
    <link rel="manifest" href="/manifest.json" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">
    <script src="https://kit.fontawesome.com/9a63fc69f8.js" crossorigin="anonymous"></script>
    <title>NACS</title>
    <link href="~/static/css/main.css" rel="stylesheet" />
    <link href="~/css/custom.css" rel="stylesheet" />

    @if (Context.Kentico().PageBuilder().EditMode)
    {
        <page-builder-styles />
    }
</head>
<body id="backToTop">
    <noscript>You need to enable JavaScript to run this app.</noscript>
    <div class="main">
        <!--  -->
        @RenderSection("GoogleAdsBeforeHeader",required:false)

        <!--NavBar-->
        @* @await Component.InvokeAsync("Navbar") *@

        @RenderBody()

        <!--Footer-->
        @* @await Component.InvokeAsync("ConvenienceFooter") *@
    </div>
    @if (Context.Kentico().PageBuilder().EditMode)
    {
        <page-builder-scripts />
    }
    <script defer="defer" src="/build/static/js/main.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/@@popperjs/core@2.5.4/dist/umd/popper.min.js"></script>
    
    @RenderSection("scripts",required:false)
</body>
</html>