                <asp:Repeater runat="server" ID="rptSubNavigation" OnItemDataBound="rptSubNavigationMobile_ItemDataBound">
                    <HeaderTemplate><ul class="col-12 col-sm-6 static sub"></HeaderTemplate>
                    <ItemTemplate>
                        <li class="static<%# Eval(" Class") %>
                            ">
                            <asp:HyperLink runat="server" ID="hypNavItem" Text='<%# Eval("Title") %>' NavigateUrl='<%# Eval("URL") %>' CssClass='static menu-item' />
                            <ul class="col-12 static third-level<%# Eval(" Class") %>
                                ">
                                <asp:Repeater runat="server" ID="rptThirdLevelNavigation">
                                    <ItemTemplate>
                                        <li class="static<%# Eval(" Class") %>
                                            ">
                                            <asp:HyperLink runat="server" ID="hypNavItem" Text='<%# Eval("Title") %>' NavigateUrl='<%# Eval("URL") %>' CssClass='static menu-item' />
                                        </li>
                                    </ItemTemplate>
                                </asp:Repeater>
                            </ul>
                        </li>
                    </ItemTemplate>
                    <FooterTemplate></ul></FooterTemplate>
                </asp:Repeater>
                <asp:Repeater runat="server" ID="rptFeaturedNavigation">
                    <HeaderTemplate><ul class="col-12 col-sm-6 static featured"></HeaderTemplate>
                    <ItemTemplate>
                        <li class="static<%# Eval(" Class") %>
                            feature">
                            <div class="desc"><%# Eval("Description") %></div>
                        </li>
                    </ItemTemplate>
                    <FooterTemplate></ul></FooterTemplate>
                </asp:Repeater>
            </div>
        </li>
    </ItemTemplate>
    <FooterTemplate></ul></FooterTemplate>
</asp:Repeater>