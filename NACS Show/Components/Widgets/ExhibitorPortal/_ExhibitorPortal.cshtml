                    <!-- Attendee Lists Purchasing and Downloads -->
                    @if (Model.PNLAttendeeListsVisible)
                    {
                        <div id="_pnlAttendeeLists" class="col-12 w-100">
                            <div class="row middle-xs center-xs pricing--card fill--gradient-pink rounded-top">
