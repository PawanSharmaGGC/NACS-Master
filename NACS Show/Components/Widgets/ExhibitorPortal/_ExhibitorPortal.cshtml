                                    <asp:Label ID="_lblDescription_AttendeeLists" runat="server"></asp:Label>
                                    &nbsp;&nbsp;
                                    <a href="javascript:openDialog('_div_moreinfo_attendeelists')" style="font-weight:bold;"><i class="fas fa-info-circle" style="margin-right:5px;"></i>What data is included?</a>
                                    <div id="_div_moreinfo_attendeelists_outside" style="display:none;">
                                        <div id="_div_moreinfo_attendeelists" style='padding:10px;'>
                                            Lists include name, title, company, address, phone, fax, registration type (i.e. Retailer, Convenience Distributor, Non-Exhibiting Supplier, etc.) and store class (range of number of stores operated). It DOES NOT include email addresses. Attendees who have opted out of receiving mailings from exhibitors will not be included in the list.
                                        </div>
                                    </div>
                                </span>
                            </div>
                            <div class="row middle-xs center-xs pricing--card fill--solid-very-light-grey rounded-bottom">
                                @if (!string.IsNullOrEmpty(Model.LBLStatusMsg_AttendeeLists))
                                {
                                    <label ID="_lblStatusMsg_AttendeeLists" class="col-xs-12 pricing--card--subtitle status">@Model.LBLStatusMsg_AttendeeLists</label>
                                }
                                <div ID="_pnlHCFreeLists">
                                    <script language="javascript">
                                        function ValidateAgreementHC(oSrc, args) {
                                            args.IsValid = false;
                                            var chk = document.getElementById(oSrc.id.replace("_val", "_cb"));
                                            var chkn = 0;

                                            if (chk.checked == true) {
                                                chk.style.outline = '';
                                                chkn++;
                                            }
                                            else {
                                                chk.style.outline = '2px solid #ec008c';
                                            }

