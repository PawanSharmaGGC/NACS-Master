        //Show/Hide panels
        function ShowHidePanel(oSrc, Answer) {
            var Panel = oSrc.id.replace("_rdo", "_pnl").replace("Yes", "").replace("No", "");
            var Panel2 = Panel + "2";

            if (Answer == "Yes") {
                document.getElementById(Panel).style.display = "";
                document.getElementById(Panel).style.visibility = "visible";
                document.getElementById(Panel2).style.display = "none";
                document.getElementById(Panel2).style.visibility = "hidden";
            }
            else if (Answer == "No") {
                document.getElementById(Panel).style.display = "none";
                document.getElementById(Panel).style.visibility = "hidden";
                document.getElementById(Panel2).style.display = "";
                document.getElementById(Panel2).style.visibility = "visible";
            }
            else {
                document.getElementById(Panel).style.display = "none";
                document.getElementById(Panel).style.visibility = "hidden";
                document.getElementById(Panel2).style.display = "";
                document.getElementById(Panel2).style.visibility = "visible";
            }
        }

        function ValidateSelections() {
            var count = 0;
            $('select.slotselections').each(function () {
                //the ",10" forces base10 counting incase of possible leading 0's that could be interpretted as base8 or some other
                count += parseInt($(this).val(), 10);
            });

            if (count == 0) {
                alert("No time slots have been selected. Click 'Go Back' to cancel.");
                return false;
            }
        }

        function SetSelfRegSlot(i) {
            var ClientID = '<%= this.ClientID %>' + "_";
            var SlotHolder = document.getElementById(ClientID + "_hdnSelfRegSlot");

            SlotHolder.value = i;
        }

        function ValSelfRegSlot() {
            var ClientID = '<%= this.ClientID %>' + "_";
            var SlotHolder = document.getElementById(ClientID + "_hdnSelfRegSlot");

            if (SlotHolder.value == "") {
                alert("Please make a selection before continuing.");
                return false;
            }
            else { return true; }
        }

