        function ValidateContactInfo() {
            var ClientID = '<%= this.ClientID %>' + "_";
            var Validator = ClientID + "_lblValidation";
            var FirstName = ClientID + "_txtVFirstName";
            var valFirstName = ClientID + "_valVFirstName";
            var LastName = ClientID + "_txtVLastName";
            var valLastName = ClientID + "_valVLastName";
            var Title = ClientID + "_txtVTitle";
            var valTitle = ClientID + "_valVTitle";
            var Email = ClientID + "_txtVEmail";
            var valEmail = ClientID + "_valVEmail";
            var Mobile = ClientID + "_txtVMobile";
            var valMobile = ClientID + "_valVMobile";

            var valString = "";
            document.getElementById(Validator).innerHTML = valString;

            if (document.getElementById(FirstName).value == "") {
                valString += "<li>First Name cannot be blank</li>";
                document.getElementById(valFirstName).style.display = '';
                document.getElementById(valFirstName).style.visibility = "visible";
            }
            else {
                document.getElementById(valFirstName).style.display = "none";
                document.getElementById(valFirstName).style.visibility = "hidden";
            }

            if (document.getElementById(LastName).value == "") {
                valString += "<li>Last Name cannot be blank</li>";
                document.getElementById(valLastName).style.display = '';
                document.getElementById(valLastName).style.visibility = "visible";
            }
            else {
                document.getElementById(valLastName).style.display = "none";
                document.getElementById(valLastName).style.visibility = "hidden";
            }

            if (document.getElementById(Title).value == "") {
                valString += "<li>Title cannot be blank</li>";
                document.getElementById(valTitle).style.display = '';
                document.getElementById(valTitle).style.visibility = "visible";
            }
            else {
                document.getElementById(valTitle).style.display = "none";
                document.getElementById(valTitle).style.visibility = "hidden";
            }

            if (document.getElementById(Email).value == "") {
                valString += "<li>Email cannot be blank</li>";
                document.getElementById(valEmail).style.display = '';
                document.getElementById(valEmail).style.visibility = "visible";
            }
            else {
                document.getElementById(valEmail).style.display = "none";
                document.getElementById(valEmail).style.visibility = "hidden";
            }

            if (document.getElementById(Mobile).value == "") {
                valString += "<li>Mobile Phone cannot be blank</li>";
                document.getElementById(valMobile).style.display = '';
                document.getElementById(valMobile).style.visibility = "visible";
            }
            else {
                document.getElementById(valMobile).style.display = "none";
                document.getElementById(valMobile).style.visibility = "hidden";
            }

            if (valString == "") {
                document.getElementById(Validator).style.display = "none";
                document.getElementById(Validator).style.visibility = "hidden";
                return true;
            }
            else {
                document.getElementById(Validator).innerHTML = valString;
                document.getElementById(Validator).style.display = '';
                document.getElementById(Validator).style.visibility = "visible";
                return false;
            }
        }

        function ValidateFile() {
            var validFilesTypes = ["bmp", "gif", "png", "jpg", "jpeg", "tif", "tiff"];
            var file = $get("<%=_fuCompanyLogo.ClientID%>");
            var label = $get("<%=_lblUploadMsg.ClientID%>");
            var path = file.value;
            var ext = path.substring(path.lastIndexOf(".") + 1, path.length).toLowerCase();

            var isValidFile = false;
            for (var i = 0; i < validFilesTypes.length; i++) {
                if (ext == validFilesTypes[i]) {
                    isValidFile = true;
                    break;
                }
            }

            if (!isValidFile) {
                label.style.color = "red";
                label.innerHTML = "Invalid File. Please select a filetype in " + validFilesTypes.join(", ");
            }
            return isValidFile;
        }

        function CheckExbSelection() {
            var ExbValue = $get("<%=_ddlExhibitors.ClientID%>").value;
            if (ExbValue == "-1") {
                alert("Please select an Exhibitor to continue");
                return false;
            }
            else { return true; }
        }
